        <slf4j.version>1.6.6</slf4j.version>

        <spring-framework.version>4.1.2.RELEASE</spring-framework.version>
        <spring-social-core.version>1.1.0.RELEASE</spring-social-core.version>
        <spring-boot.version>1.2.0.RELEASE</spring-boot.version>

        <jetty.version>9.1.5.v20140505</jetty.version>

        <cglib.version>2.2.2</cglib.version>
        <args4j.version>2.0.16</args4j.version>
        <guava.version>18.0</guava.version>


        <commons-lang3.version>3.1</commons-lang3.version>
        <commons-io.version>2.3</commons-io.version>
        <commons-codec.version>1.6</commons-codec.version>
        <commons-pool.version>1.6</commons-pool.version>
        <commons-validator.version>1.4.0</commons-validator.version>

        <mokito.version>1.9.5-rc1</mokito.version>
        <junit.version>4.10</junit.version>

        <metrics.version>3.0.1</metrics.version>

        <hibernate-validator.version>5.1.0.Final</hibernate-validator.version>
        <javax-validation-api.version>1.1.0.Final</javax-validation-api.version>

        <xmemcache.version>2.0.0</xmemcache.version>

        <maven-compiler-plugin.version>2.5.1</maven-compiler-plugin.version>
        <maven-war-plugin.version>2.4</maven-war-plugin.version>
        <maven-release-plugin.version>2.3.2</maven-release-plugin.version>
        <maven-source-plugin.version>2.1.2</maven-source-plugin.version>
        <versions-maven-plugin.version>1.3.1</versions-maven-plugin.version>

        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <java.version>1.7</java.version>
        <jedis.version>2.7.0</jedis.version>
        <target.version>1.7</target.version>
        <lombok.version>1.14.8</lombok.version>
        <amazon-aws.version>1.9.3</amazon-aws.version>
        <log4jdbc.log4j2.version>1.16</log4jdbc.log4j2.version>
        <rest.assured.version>2.3.3</rest.assured.version>
        <commons-lang3.version>3.3.2</commons-lang3.version>
        <junit.version>4.12</junit.version>
        <spring.version>4.1.6.RELEASE</spring.version>
