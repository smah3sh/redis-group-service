    <name>Simple Group Service</name>
    <packaging>jar</packaging>
    <version>0.0.1-SNAPSHOT</version>
    <description>Implementation of groups backed by Redis cluster</description>


    <properties>
        <docker-maven.version>2.0.0-SNAPSHOT</docker-maven.version>
        <bson4jackson.version>2.2.0</bson4jackson.version>
        <jackson.version>2.2.3</jackson.version>
